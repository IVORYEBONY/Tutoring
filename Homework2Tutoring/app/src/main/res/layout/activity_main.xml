<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.doug_mac.homework2tutoring.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textViewActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Activity 1"
        android:textSize="30sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="66dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewString1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="String 1:"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextString1Content" />

    <EditText
        android:id="@+id/editTextString1Content"
        android:layout_width="165dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintRight_toRightOf="@+id/textViewString2Content"
        app:layout_constraintBaseline_toBaselineOf="@+id/buttonString1"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textViewString2Content"
        tools:layout_editor_absoluteY="175dp"
        tools:layout_editor_absoluteX="89dp" />

    <Button
        android:id="@+id/buttonString1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickEnter"
        android:text="Enter"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/buttonString2"
        app:layout_constraintRight_toRightOf="@+id/buttonString2"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="23dp"
        app:layout_constraintLeft_toLeftOf="@+id/buttonString2" />

    <TextView
        android:id="@+id/textViewString2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="String 2:"
        android:layout_marginStart="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewString2Content"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />


    <TextView
        android:id="@+id/textViewString2Content"
        android:layout_width="0dp"
        android:layout_height="47dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/buttonString2"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textViewString3Content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="26dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="19dp"
        app:layout_constraintLeft_toRightOf="@+id/textViewString2" />

    <Button
        android:id="@+id/buttonString2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickGoActivity2"
        android:text="Go"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/buttonString3"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="18dp" />

    <TextView
        android:id="@+id/textViewString3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="String 3:"
        android:layout_marginStart="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewString3Content"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textViewString3Content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="TextView"
        android:textSize="18sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/buttonString3"
        app:layout_constraintRight_toRightOf="@+id/textViewString2Content"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textViewString2Content"
        app:layout_constraintTop_toTopOf="@+id/buttonString3" />

    <Button
        android:id="@+id/buttonString3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickGoActivity3"
        android:text="Go"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/buttonString2"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="144dp"
        app:layout_constraintLeft_toLeftOf="@+id/buttonString2" />


</android.support.constraint.ConstraintLayout>
